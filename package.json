{
  "name": "TRPI",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start:all": "turbo run start:all",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "run:client" : "turbo run run:client",
    "run:web" : "turbo run run:web"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.20.0"
  },
  "dependencies": {
    "@nestjs/cli": "^9.2.0"
  },
  "packageManager": "yarn@1.22.19"
}
